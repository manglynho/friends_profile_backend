{
    "compilerOptions": {
      "target": "ES6",
      "outDir": "./build/",
      "module": "commonjs",
      "types": ["node", "jest"],
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": false,
      "noFallthroughCasesInSwitch": true,
      "esModuleInterop": true,
      "resolveJsonModule": true      
    }
  }